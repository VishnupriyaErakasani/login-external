pipeline{
    agent any 
    stages{
        stage('archive'){
            steps{
                archiveArtifacts artifacts: '**/target/*.jar', fingerprint: true
                junit'/target/surefire-reports/*.xml'
            }
        }
        stage('Show login page username'){
            steps{
            echo "Username: ${USERNAME}"

        }
}
       stage('Show login page password'){
            steps{
            echo "password: ${password}"
        
        }
}
    }
}
